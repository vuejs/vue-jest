{
  "name": "vue3-babel-in-package",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest --no-cache test.js"
  },
  "dependencies": {
    "vue": "^3.2.22"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "coffeescript": "^2.3.2",
    "jest": "^28.0.0",
    "jest-environment-jsdom": "28.0.2",
    "ts-jest": "^28.0.1",
    "typescript": "^4.6.4",
    "@vue/vue3-jest": "^28.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "@vue/vue3-jest"
    }
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  }
}
